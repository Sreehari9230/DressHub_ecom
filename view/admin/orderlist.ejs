            <section class="content-main">
                <div class="content-header">
                    <div>
                        <h2 class="content-title card-title">Order List</h2>
                        <p>Lorem ipsum dolor sit amet.</p>
                    </div>
                    <div>
                        <input type="text" placeholder="Search order ID" class="form-control bg-white" />
                    </div>
                </div>
                <div class="card mb-4">
                    <!-- <header class="card-header">
                        <div class="row gx-3">
                            <div class="col-lg-4 col-md-6 me-auto">
                                <input type="text" placeholder="Search..." class="form-control" />
                            </div>
                            <div class="col-lg-2 col-6 col-md-3">
                                <select class="form-select">
                                    <option>Status</option>
                                    <option>Active</option>
                                    <option>Disabled</option>
                                    <option>Show all</option>
                                </select>
                            </div>
                            <div class="col-lg-2 col-6 col-md-3">
                                <select class="form-select">
                                    <option>Show 20</option>
                                    <option>Show 30</option>
                                    <option>Show 40</option>
                                </select>
                            </div>
                        </div>
                    </header> -->
                    <!-- card-header end// -->
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>Order ID</th>
                                        <th>Price</th>
                                        <th>PaymentMethod</th>
                                        <th>Status</th>
                                        <th class="action-column">Actions</th>
                                        <th>View</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(Orders.length> 0){ %>
                                      <% for(let i=0; i < Orders.length; i++){ %>
                                        <tr>
                                          <td>
                                            <%= i + 1 %>
                                          </td>
                                          <td>
                                            <%= Orders[i]._id %>
                                          </td>
                                          <td>
                                            $<%= Orders[i].subtotal  %>
                                          </td>
                                          <td>
                                            <%= Orders[i].paymentMethod %>
                                          </td>
                                          <td>
                                            <% if (
                                            Orders[i].status === 'placed' ||
                                            Orders[i].status === 'delivered' ||
                                            Orders[i].status === 'waiting for approvel' ||
                                            Orders[i].status === 'Return Approved'
                                            ){ %>
                                              <a href="/admin/orderstatus?id=<%= Orders[i]._id %>">
                                                <span class="status-label status-placed">
                                                <%= Orders[i].status %>
                                            </span></a>
                                            <%}else if(Orders[i].status === 'cancelled') {%>
                                              <a>
                                                <span class="status-canceled px-3 py-1 text-xs font-semibold leading-tight text-gray-800 bg-gray-200 rounded-full dark:text-gray-100 dark:bg-gray-700">
                                                    <%= Orders[i].status %>
                                                </span>
                                            </a>
                                            <%}else if(Orders[i].status === 'pending') {%>
                                            <a href="/admin/orderstatus?id=<%= Orders[i]._id %>">
                                              <span class="status-canceled px-3 py-1 text-xs font-semibold leading-tight text-gray-800 bg-gray-200 rounded-full dark:text-gray-100 dark:bg-gray-700">
                                                  <%= Orders[i].status %>
                                              </span>
                                          </a>
                                          <% } %>
                                        </td>
                                        <td>
                                          <% if(Orders[i].status == 'cancelled'){ %>
                                            <a class="px-2 py-1 font-semibold leading-tight text-orange-700 bg-orange-100 rounded-full dark:text-white dark:bg-orange-600">
                                              Canceled
                                          </a>
                                          <%}else if(
                                            Orders[i].status !== 'delivered' &&
                                            Orders[i].status !== 'waiting for approvel' &&
                                            Orders[i].status !== 'Return Approved' &&
                                            Orders[i].status !== 'pending'
                                          ){ %>
                                            <a href="/admin/ordercancel?id=<%= Orders[i]._id%>">
                                              <span class="btn btn-sm font-sm btn-light rounded">Cancel</span>
                                              </a>
                                              <% if(
                                                Orders[i].status == 'placed' ||
                                                Orders[i].status == 'delivered' &&
                                                Orders[i].status !== 'delivered' &&
                                                Orders[i].status !== 'pending'
                                              ) { %>
                                               <a href="/admin/orderdelivered?id=<%= Orders[i]._id%>">
                                              <span class="btn btn-sm font-sm rounded btn-brand">Delivered</span>
                                          </a>
                                          <%}%>
                                          <% } else if(Orders[i].status == 'Return Approved') { %>
                                            <a class="px-2 py-1 font-semibold leading-tight text-orange-700 bg-orange-100 rounded-full dark:text-white dark:bg-orange-600">
                                              Approved
                                              </a>
                                          <% }else if(
                                            Orders[i].status !== 'delivered' &&
                                            Orders[i].status !== 'pending'
                                            
                                          ) { %>
                                            <a href="/admin/orderdelivered?id=<%= Orders[i]._id%>">
                                              <span class="btn btn-sm font-sm rounded btn-brand">Approve</span>
                                          </a>
                                          <% } %>
                                          </td>
                                          <td>
                                            <span class="btn btn-sm font-sm rounded btn-brand">View Details</span>
                                            </td>
                                          
                                         
                                        </tr>
                                        <% } %>
                                          <% } else { %>
                                            <tr>
                                              <td colspan="5">Orders Not Found</td>
                                            </tr>
                                            <% } %>
                                  </tbody>
                            </table>
                        </div>
                        <!-- table-responsive //end -->
                    </div>
                    <!-- card-body end// -->
                </div>
                <!-- card end// -->
